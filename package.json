    },
    "./nav": {
      "types": "./dist/nav/index.d.ts",
      "import": "./dist/nav/index.js",
      "svelte": "./dist/nav/index.js"
    },
    "./pill": {
      "types": "./dist/pill/index.d.ts",
      "import": "./dist/pill/index.js",
