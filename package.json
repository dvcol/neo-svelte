    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js",
      "svelte": "./dist/providers/index.js"
    },
    "./skeletons": {
      "types": "./dist/skeletons/index.d.ts",
      "import": "./dist/skeletons/index.js",
      "svelte": "./dist/skeletons/index.js"
    },
    "./stepper": {
      "types": "./dist/stepper/index.d.ts",
      "import": "./dist/stepper/index.js",
      "svelte": "./dist/stepper/index.js"
    },
    "./text": {
      "types": "./dist/text/index.d.ts",
      "import": "./dist/text/index.js",
      "svelte": "./dist/text/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "!dist/**/*.spec.*",
    "!dist/**/*.test.*",
    "*.md",
    "LICENSE",
    "dist"
  ],
  "engines": {
    "node": ">=20",
    "pnpm": ">= 8"
  },
  "svelte": "./dist/index.js",
  "scripts": {
    "prepare": "husky",
    "vite:serve": "vite --host",
    "vite:build": "vite build",
    "vite:watch": "vite build --watch",
    "vite:preview": "vite preview --host",
    "dev": "run-s vite:serve",
    "build": "run-s check package",
    "build:web": "VITE_MODE=WEB run-s vite:build",
    "serve:web": "VITE_MODE=WEB run-p vite:watch vite:preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "run-s package",
    "check:type": "tsc -p tsconfig.build.json",
    "check:type:watch": "tsc -p tsconfig.build.json --watch",
    "check:svelte": "svelte-kit sync && svelte-check --tsconfig tsconfig.build.json",
    "check:svelte:watch": "svelte-kit sync && svelte-check --tsconfig tsconfig.build.json --watch",
    "check": "run-s check:type check:svelte",
    "check:watch": "run-p check:type:watch check:svelte:watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "style": "stylelint **/*.{svelte,css,scss,less,html} --go '{\"gitignore\":true}'",
    "style:fix": "pnpm run style --fix",
    "test:unit": "vitest --run --coverage",
    "test:watch": "vitest",
    "release": "standard-version",
    "release:changelog": "extract-changelog-release > RELEASE.md"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "svelte": ">=5"
  },
  "dependencies": {
    "@dvcol/common-utils": "^1.29.4",
    "@dvcol/svelte-utils": "^1.17.1",
    "@skeletonlabs/floating-ui-svelte": "^0.3.9",
    "svelte": "^5.27.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@dvcol/eslint-config": "^1.2.0",
    "@dvcol/stylelint-plugin-presets": "^2.1.2",
    "@dvcol/svelte-simple-router": "^1.11.2",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/node22": "^22.0.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/dom-view-transitions": "^1.0.6",
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.24.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-svelte": "^3.5.1",
    "extract-changelog-release": "^1.0.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.1",
    "navigation-api-types": "^0.6.1",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.12",
    "sass": "^1.86.3",
    "standard-version": "^9.5.0",
    "stylelint": "^16.18.0",
    "svelte-check": "^4.1.6",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.8.3",
    "vite": "^6.2.1",
    "vite-plugin-checker": "^0.9.1",
    "vitest": "^3.1.1"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,jsx,ts,tsx,vue,svelte,json,md,yml,html,md,svg,xml}": [
      "eslint --fix"
    ],
    "*.{css,scss,less,vue,svelte}": [
      "stylelint --fix"
    ],
    "*.{svelte}": [
      "pnpm run check:svelte"
    ]
  }
}
